CC = gcc
SRC = main.c holberton.c
OBJ = $(SRC:.c=.o)
NAME = holberton
RM = rm -f
CFLAGS = -Wall -Werror -Wextra -pedantic
all: $(OBJ)
	$(CC) $(CFLAGS) $(OBJ) -o $(NAME)
clean:
	$(RM) *~ $(NAME)
oclean:
	$(RM) $(OBJÃ)
fclean: clear oclean
re: oclean $(OBJ)
	$(CC) $(CFLAGS) $(OBJ) -o $(NAME)
.PHONY: clean fclean
